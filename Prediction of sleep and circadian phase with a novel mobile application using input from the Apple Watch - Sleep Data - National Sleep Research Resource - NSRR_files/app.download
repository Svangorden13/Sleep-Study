              enabled: (json['stacking'] ? false : true)
            },
            stacking: json['stacking']
          }
        },
        series: json['series']
      });
    } else {
      return $('#chart-container').html('');
    }
  };

  this.toggleVariableButtonClasses = function(element) {
    $('[data-object~="variable-chart-button"]').removeClass('btn-primary').addClass('btn-light');
    return $("[data-object~='variable-chart-button'][data-chart-type~='" + ($(element).data('chart-type')) + "']").addClass('btn-primary');
  };

  this.variablesReady = function() {
    var chart_type;
    Highcharts.setOptions({
      lang: {
        thousandsSep: ','
      },
      colors: ['#7cb5ec', '#90ed7d', '#f7a35c', '#8085e9', '#f15c80', '#e4d354', '#2b908f', '#f45b5b', '#91e8e1']
    });
    chart_type = $('#chart_type').val() || 'histogram';
    drawChart(chart_type);
    return $("[data-chart-name~='" + chart_type + "']").show();
  };

  this.nonStandardClick = function(event) {
    return event.which > 1 || event.metaKey || event.ctrlKey || event.shiftKey || event.altKey;
  };

  $(document).on('click', '[data-chart-type]', function() {
    var chart_type;
    toggleVariableButtonClasses(this);
    chart_type = $(this).data('chart-type');
    drawChart(chart_type);
    $('[data-chart-name]').hide();
    $("[data-chart-name~='" + chart_type + "']").show();
    return false;
  }).on('click', '[data-link]', function(e) {
    if ($(e.target).is('a')) {

    } else if (nonStandardClick(e)) {
      window.open($(this).data('link'));
      return false;
    } else {
      return Turbolinks.visit($(this).data('link'));
    }
  }).keydown(function(e) {
    if (e.which === 39 && !$('input, textarea, select, a').is(':focus')) {
      if ($('#next-variable')[0]) {
        return $('#next-variable')[0].click();
      }
    } else if (e.which === 37 && !$('input, textarea, select, a').is(':focus')) {
      if ($('#previous-variable')[0]) {
        return $('#previous-variable')[0].click();
      }
    }
  });

}).call(this);
